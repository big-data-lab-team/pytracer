{
    "modules_to_load": [
        "math",
        "numpy",
        "sklearn"
    ],
    "include_file": "",
    "exclude_file": [
        "exclude.default.txt",
        "exclude.numpy.default.txt",
        "exclude.sklearn.default.txt"
    ],
    "filter_alias": true,
    "logger": {
        "format": "print",
        "output": "pytracer.log",
        "color": false,
        "level": "error"
    },
    "io": {
        "type": "pickle",
        "backtrace": true,
        "cache": {
            "root": ".__pytracercache__"
        }
    },
    "numpy": {
        "ufunc": true
    }
}